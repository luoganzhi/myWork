#import "NSArray+Helper.h"

@implementation NSArray (Helper)

+(BOOL)isNullArray:(NSArray*)array{
    if (array == nil || [array isEqual:[NSNull null]] || array.count == 0) {
        
        return YES;
        
    }else  {
        
        return NO;
        
    }

}
-(BOOL)isNULL
{
    if (self==nil|| [self isEqual:[NSNull null]] || self.count == 0) {
        
        return YES;
        
    }else  {
        
        return NO;
    
    }

}
@end
